{% set domain = domain ?? siteUrl|trim('/') %}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
	<head>
    </head>
	<body
		width="100%"
		style="margin: 0; padding: 0 !important; mso-line-height-rule: exactly;">
		<center 
			aria-roledescription="email" 
			lang="en"
			role="article"
			style="width: 100%;">

			<!-- Email Wrapper: STARTS -->
			<table align="center" role="presentation" cellspacing="0" cellpadding="0"
			border="0" width="600" style="margin: auto;" class="email-wrapper">
				<tbody>
					{% block content %}{% endblock %}
					{% block actionContent %}{% endblock %}
					{% block emailFooter %}{% endblock %}
				</tbody>
			</table>
			<!-- Email Container: ENDS -->
		</center>
	</body>
</html>