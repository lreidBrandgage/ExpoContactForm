{
	"name": "brandgage/expo-contact-form",
	"description": "General Contact Form plugin with email",
	"type": "craft-plugin",
	"license": "MIT",
	"version": "1.0",
	"authors": [{
		"name": "Lucas Reid",
		"email": "lreid@brandgage.com"
	}],
	"require": {
		"php": ">=8.1.0",
		"craftcms/cms": "^5.1.0"
	},
	"autoload": {
		"psr-4": {
			"brandgage\\expo-contact-form\\": "src/"
		}
	},
	"extra": {
		"handle": "ecf",
		"name": "Expo Contact Form",
		"developer": "lreid@brandgage.com",
		"documentationUrl": "???",
		"class": "brandgage\\ecf\\ExpoContactForm"
	},
	"config": {
		"sort-packages": true,
		"platform": {
			"php": "8.2"
		},
		"allow-plugins": {
			"yiisoft/yii2-composer": true,
			"craftcms/plugin-installer": true
		}
	}
}